{
    "compilerOptions": {
        "outDir": "./dist/",
        "typeRoots": [
            "node_modules/@types"
        ],
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es6",
        "types": ["jest","googlemaps"],
        "jsx": "react"
    },

    "include": ["src"]
}
